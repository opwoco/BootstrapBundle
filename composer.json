{
    "name": "opwoco/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Easy integration of twitters bootstrap into symfony2",
    "keywords": ["form", "extra form", "bootstrap", "bootstrap form", "template"],
    "license": "MIT",
    "authors": [
        {
            "name": "opwoco",
            "email": "info@opwoco.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/opwoco/BootstrapBundle/contributors"
        }
    ],
    "autoload": {
        "psr-0": {
            "opwoco\\BootstrapBundle\\": ""
        }
    },
    "require": {
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/console": "~2.3",
        "twig/twig": "^1.14.2",
        "mopa/composer-bridge": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6"
    },
    "conflict": {
        "symfony/twig-bridge": "<2.3"
    },
    "suggest":  {

        "twbs/bootstrap": ">2.0,<4.0-dev",
        "knplabs/knp-paginator-bundle": "~2.3",
        "knplabs/knp-menu-bundle": "~2.0@dev",
        "craue/formflow-bundle": "~2.0",
        "twbs/bootstrap-sass": "v3.1.1",
        "sp/bower-bundle": "0.11.*"
    },
    "scripts": {
        "post-install-cmd": [
            "opwoco\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
            "opwoco\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]

    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
